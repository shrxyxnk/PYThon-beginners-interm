#here is a simple Python 3 code that Performs arithmetic operations on numbers:

# Addition
result_addition = 5 + 3
print("5 + 3 =", result_addition)

# Subtraction
result_subtraction = 7 - 2
print("7 - 2 =", result_subtraction)

# Multiplication
result_multiplication = 4 * 6
print("4 * 6 =", result_multiplication)

# Division
result_division = 15 / 3
print("15 / 3 =", result_division)

# Integer Division (Floor Division)
result_floor_division = 17 // 5
print("17 // 5 =", result_floor_division)

# Modulo (Remainder)
result_modulo = 17 % 5
print("17 % 5 =", result_modulo)

# Exponentiation
result_exponentiation = 2 ** 4
print("2 ** 4 =", result_exponentiation)
